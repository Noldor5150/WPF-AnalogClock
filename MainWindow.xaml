<Window x:Class="AnalogClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnalogClock"
        xmlns:custom="clr-namespace:AnalogClock.CustomControls"
        mc:Ignorable="d"
        Title="AnalogClock" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center">
        <custom:AnalogClock ShowSeconds="True" TimeChanged="AnalogClock_TimeChanged"/>
        <custom:DigitalClock ShowSeconds="True" ColonBlink="True" HorizontalAlignment="Center" Margin="10" FontSize="50"/>
    </StackPanel>
</Window>
